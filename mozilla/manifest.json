{
  "manifest_version": 2,
  "name": "ShortsVolCtrl",
  "version": "1.0",
  "description": "Wake up biden, we need volume control in shorts",
  "applications": {
    "gecko": {
      "id": "shorts.vol.ctrl@polymorphous.org",
      "strict_min_version": "56.0"
    }
  },
  "icons": {
    "256": "icons/icon256.png"
  },
    "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "<all_urls>", 
    "tabs", 
    "activeTab",
    "scripting"
]

}